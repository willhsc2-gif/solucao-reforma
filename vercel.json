{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@vite_supabase_url",
      "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key"
    }
  },
  "cleanUrls": false,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/index.html",
      "headers": { "Cache-Control": "no-cache, no-store, must-revalidate", "Content-Type": "text/html" },
      "dest": "/index.html"
    },
    {
      "src": "/workers/pdf.worker.min.js",
      "headers": { "Content-Type": "application/javascript", "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/workers/pdf.worker.min.js"
    },
    {
      "src": "/assets/pdf.worker.min-([a-zA-Z0-9]+)\\.js",
      "headers": { "Content-Type": "application/javascript", "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/assets/pdf.worker.min-$1.js"
    },
    {
      "src": "/assets/index-([a-zA-Z0-9]+)\\.js",
      "headers": { "Content-Type": "application/javascript", "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/assets/index-$1.js"
    },
    {
      "src": "/assets/index-([a-zA-Z0-9]+)\\.css",
      "headers": { "Content-Type": "text/css", "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/assets/index-$1.css"
    },
    {
      "src": "/assets/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/icons/(.*\\.png)",
      "headers": { "Content-Type": "image/png", "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/icons/$1"
    },
    {
      "src": "/manifest.json",
      "headers": { "Content-Type": "application/manifest+json", "Cache-Control": "public, max-age=0, must-revalidate" },
      "dest": "/manifest.json"
    },
    {
      "src": "/service-worker.js",
      "headers": { "Content-Type": "application/javascript", "Cache-Control": "public, max-age=0, must-revalidate" },
      "dest": "/service-worker.js"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}